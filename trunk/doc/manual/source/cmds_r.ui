!begin_node (!kw [rtf_monofont])
!html_name cmd_rtf_monofont.html

!iflang [german]
Namen des Monospaced-Font für RTF setzen.
!endif
!iflang [english]
Set name of monospaced font (RTF only).
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!PREAMBLE)

!item [(!SYNTAX)] (!T)!/rtf_monofont <fontname>(!t)

!iflang [german]
!item [(!DESCRIPTION)]
Mit diesem Kommando kann man den Font setzen, der zur
Darstellung von Klartext benutzt werden soll. Als
Default verwendet UDO den Font ""Courier New"".
!endif
!iflang [english]
!item [(!DESCRIPTION)]
With this command you can set the font that will be used to display
preformatted text. The default is ""Courier New"".
!endif

!item [(!EXAMPLE)]
(!T)!/rtf_monofont Monospace 821(!t)

!item [(!SEEALSO)]
(!KW [rtf_propfont]), (!KW [rtf_monofont_size])

!end_xlist

