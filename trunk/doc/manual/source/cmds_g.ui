################################################################################
!begin_node G!..
!html_name cmd-g.html

################################################################################
!ifset OldKeywords
!begin_node (!kw [german])
!html_name cmd_german.html

!iflang [german]
Sprache setzen.
!endif
!iflang [english]
Define language.
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!PREAMBLE)

!item [(!SYNTAX)] (!T)!/german(!t)

!item [(!DESCRIPTION)]
(!NOTEXISTING)
# Der Befehl (!KW [german]) existiert nicht mehr.
(!USEINSTEAD[]) (!KW [language] [[german!!]]).

!item [(!EXISTEDUNTIL)] Release~6 Patchlevel~1

!item [(!SEEALSO)]
(!KW [language])

!end_xlist

!end_node
!endif

