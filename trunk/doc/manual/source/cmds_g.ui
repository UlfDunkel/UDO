################################################################################
!begin_node (!kkw [grin])
!html_name cmd_grin().html

!iflang [german]
Emoticon ausgeben.
!endif
!iflang [english]
Print an emoticon.
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!PLACEHOLDER), (!PREAMBLE) & (!MAINPART)

!item [(!SYNTAX)] (!T)(!/grin)(!t)

!iflang [german]
!item [(!DESCRIPTION)]
Dieser Platzhalter wird durch einen grinsenden Smiley
ersetzt, der in Schreibmaschinenschrift ausgegeben wird.
!endif
!iflang [english]
!item [(!DESCRIPTION)]
(!/grin) will be replaced by a grinning emoticon displayed in typewriter.
!endif

!iflang [german]
!item [(!EXAMPLE)]
Aus (!T)(!/grin)(!t) wird (!grin).
!endif
!iflang [english]
!item [(!EXAMPLE)]
(!T)(!/grin)(!t) becomes (!grin).
!endif

!item [(!SEEALSO)]
(!KKW [laugh])

!end_xlist

!end_node

################################################################################
!end_node

