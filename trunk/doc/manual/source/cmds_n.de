(!KW [index]), (!KKW [index]), (!KKW [idx])

!end_xlist

(!EXAMPLE)
(!EXAMPLE_BEGIN)
!begin_verbatim
!no_index [rtf]
!end_verbatim
(!EXAMPLE_END)

!end_node

