!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!MAINPART)

!item [(!SYNTAX)] (!T)!/chaptericon_text <(!TEXT)>(!t)

!iflang [german]
!item [(!DESCRIPTION)]
Hiermit kann man einen Text unterhalb eines Chaptericons darstellen.
!endif
!iflang [english]
!item [(!DESCRIPTION)]
""<text>"" will be displayed below the graphic of a (!KW [chaptericon]).
!endif

!item [(!EXISTSSINCE)] Release~6 Patchlevel~7

!item [(!SEEALSO)]
(!KW [html_modern_layout]), (!KW [html_frames_layout]), (!KW [chaptericon]), (!KW [chaptericon_active])

!end_xlist

!end_node

################################################################################
!begin_node (!kw [chapterimage])
!html_name cmd_chapterimage.html

!iflang [german]
Kapitelüberschrift durch Grafik ersetzen.
!endif
!iflang [english]
Replace chapter title with graphic.
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!MAINPART)

!item [(!SYNTAX)] (!T)!/chapterimage <(!FILE)>(!t)

!iflang [german]
!item [(!DESCRIPTION)]
Beim HTML-, ST-Guide- und WinHelp-Format wird
anstelle einer Kapitelüberschrift die Grafik
""<datei>"" verwendet, falls im Vorspann der Schalter
(!KW [use_chapter_images]) verwendet wurde.
!endif
!iflang [english]
!item [(!DESCRIPTION)]
Converting to Windows Help, HTML or ST-Guide the image called
''<file>'' is displayed instead of the chapter title if
(!KW [use_chapter_images]) is used inside the preamble
!endif

!item [(!EXAMPLE)]
(!T)!/chapterimage udo(!t)

!item [(!EXISTSSINCE)] Release~4 Version~0.43

!iflang [german]
!item [(!SEEALSO)]
(!link [Bilder] [Bilder]), (!KW [use_chapter_images])
!endif
!iflang [english]
!item [(!SEEALSO)]
!endif

!end_xlist

!end_node

################################################################################
!begin_node (!kw [cinclude])
!html_name cmd_cinclude.html

!iflang [german]
Datei einbinden und in einer comment-Umgebung darstellen.
!endif
!iflang [english]
Include file in a comment environment.
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!MAINPART)

!item [(!SYNTAX)] (!T)!/cinclude <(!FILE)>(!t)

!iflang [german]
!item [(!DESCRIPTION)]
Öffnet die Datei namens ""<datei>"" und gibt deren Inhalt
als Kommentar an der Stelle aus, an der dieses
Kommando benutzt wurde. Etwaige Einrückungen werden
dabei berücksichtigt, Zentrierungen jedoch nicht.
Tabulatoren werden abhängig von (!KW [tabwidth])
umgewandelt. Enthält der Dateiname Leerzeichen,
muss er in Anführungsstriche gesetzt werden.
!endif
!iflang [english]
!item [(!DESCRIPTION)]
""<file>"" will be included and displayed like preformatted text
inside a comment environment. If you use this commands inside
another environment UDO indents the contents of this file. UDO
converts tabs according to (!KW [tabwidth]).
!endif

!item [(!EXAMPLE)]
(!T)!/cinclude hello.txt(!t)

!iflang [german]
!item [(!SEEALSO)]
(!KW [include]), (!KW [begin_comment]), (!KW [comment]), (!link [Verteilte Dokumente] [Verteilte Dokumente]), (!link [comment-Umgebung] [comment-Umgebung])
!endif
!iflang [english]
!item [(!SEEALSO)]
(!KW [include]), (!KW [begin_comment]), (!KW [comment]), (!link [Split documents] [Split documents]), (!link [comment environment] [comment environment])
!endif

!end_xlist

!end_node

################################################################################
!begin_node (!kw [code])
!html_name cmd_code.html

!iflang [german]
Auf einen anderen Zeichensatz umschalten.
!endif
!iflang [english]
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!PREAMBLE) & (!MAINPART)

!item [(!SYNTAX)] (!T)!/code [(!CODE)](!t)

!iflang [german]
!item [(!DESCRIPTION)]
UDO erwartet nach diesem Kommando Texte oder
Textpassagen, die in dem entsprechenden Zeichensatz verfasst
wurden. Um wieder auf den Systemzeichensatz
zurückzuwechseln, verwenden Sie dieses Kommando noch einmal
mit dem Namen des Systemzeichensatzes.

Hier eine Übersicht, welches Kürzel für den Zeichensatz benutzt
werden kann:
!endif
!iflang [english]
!item [(!DESCRIPTION)]
UDO expects files written in the IBM-PC character set after this
command. You can switch back to your system charset with by
using this command again with name of your system character set.

Below is an overview of the character sets UDO knows about:
!endif

!include charsets.ui

!item [(!EXISTSSINCE)] Release~6 Patchlevel~2

!iflang [german]
!item [(!SEEALSO)]
(!link [Sonderzeichen] [Sonderzeichen])
!endif
!iflang [english]
!item [(!SEEALSO)]
(!link [Special characters] [Special characters])
!endif

!end_xlist

!end_node

