################################################################################
!begin_node (!kw [pdf_high_compression])
!html_name cmd_pdf_high_compression.html

!iflang [german]
PDF-Datei komprimieren (hohe Kompressionsrate).
!endif
!iflang [english]
Compress PDF file (high compression rate).
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!PREAMBLE)

!item [(!SYNTAX)] (!T)!/pdf_high_compression(!t)

!iflang [german]
!item [(!DESCRIPTION)]
Der Einsatz dieses Schalters führt bei der späteren
Umwandlung durch PDFLaTeX zur
Komprimierung der Ausgabedatei mit einer
Kompressionsrate von ca. 50~%. Die Komprimierung
dauert relativ lange. Fast gleich gute Ergebnisse bei
kürzerer Umwandlungsdauer erzielt man mit dem
Schalter (!KW [pdf_medium_compression]).
!endif
!iflang [english]
!item [(!DESCRIPTION)]
If you use this switch inside the preamble PDFLaTex
will compress the output file by about 50~%. The compression of
the file takes some time. A similar result with a faster
compression you will get with (!KW [pdf_medium_compression]).
!endif

!item [(!SEEALSO)]
(!KW [pdf_medium_compression])

!end_xlist

!end_node

################################################################################
!begin_node (!kw [pdf_medium_compression])
!html_name cmd_pdf_medium_compression.html

!iflang [german]
PDF-Datei komprimieren (mittlere Kompressionsrate).
!endif
!iflang [english]
Compress PDF file (lower compression rate).
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!PREAMBLE)

!item [(!SYNTAX)] (!T)!/pdf_medium_compression(!t)

!iflang [german]
!item [(!DESCRIPTION)]
Der Einsatz dieses Schalters führt bei der späteren
Umwandlung durch den PDFLaTeX zur
Komprimierung der Ausgabedatei mit einer
Kompressionsrate von ca. 40~%. Bessere Ergebnisse bei
allerdings längerer Umwandlungsdauer erzielt man mit
dem Schalter (!KW [pdf_high_compression]).
!endif
!iflang [english]
!item [(!DESCRIPTION)]
If you use this switch inside the preamble PDFLaTeX
will compress the output file by about 40~%. A better result
with a slower compression you will get with
(!KW [pdf_high_compression]).
!endif

!item [(!SEEALSO)]
(!KW [pdf_high_compression])

!end_xlist

!end_node

################################################################################
!begin_node (!kw [pinclude])
!html_name cmd_pinclude.html

!iflang [german]
Datei einbinden und in einer sourcecode-Umgebung darstellen.
!endif
!iflang [english]
Include file in sourcecode environment.
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!COMMAND), (!MAINPART)

!item [(!SYNTAX)] (!T)!/pinclude <(!FILE)>(!t)

!iflang [german]
!item [(!DESCRIPTION)]
Öffnet die Datei namens ""<datei>"" und gibt deren Inhalt
als Sourcecode an der Stelle aus, an der dieses
Kommando benutzt wurde. Etwaige Einrückungen werden
dabei berücksichtigt, Zentrierungen jedoch nicht.
Tabulatoren werden abhängig von (!KW [tabwidth])
umgewandelt. Enthält der Dateiname Leerzeichen,
muß er in Anführungsstriche gesetzt werden.
!endif
!iflang [english]
!item [(!DESCRIPTION)]
""<file>"" will be included and displayed like preformatted text
inside a sourcecode environment. If you use this command inside
another environment UDO indents the contents of this file. UDO
converts tabs according to (!KW [tabwidth]).
!endif

!item [(!EXAMPLE)]
(!T)!/pinclude hello.pas(!t)

!iflang [german]
!item [(!SEEALSO)]
(!KW [tabwidth]), (!KW [include]), (!KW [sinclude]), (!link [Verteilte Dokumente] [Verteilte Dokumente]), (!link [sourcecode-Umgebung] [sourcecode-Umgebung])
!endif
!iflang [english]
!item [(!SEEALSO)]
(!KW [tabwidth]), (!KW [include]), (!KW [sinclude]), (!link [Split documents] [Split documents]), (!link [sourcecode environment] [sourcecode environment])
!endif

!end_xlist

!end_node

