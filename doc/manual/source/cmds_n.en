(!KW [use_compressed_enumerates]), 
(!KW [use_compressed_envs]), 
(!KW [use_compressed_itemizes]), 
(!KW [use_compressed_lists])

!end_xlist

(!EXAMPLE)
(!EXAMPLE_BEGIN)
!begin_verbatim
# preamble
!use_compressed_envs

# document
# [...]
!begin_itemize !not_compressed
# [...]
!end_verbatim
(!EXAMPLE_END)

