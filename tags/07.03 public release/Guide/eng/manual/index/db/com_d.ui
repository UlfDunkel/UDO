!subsubnode !/docinfo [author]

!begin_xlist [Type & position:]

!item [Type & position:]
	command, preamble

!item [Syntax:]
	(!T)!/author <text>(!t)

!item [Description:]
	""<text>"" will be used as the name of the author on the title page   
	and for different other purposes.                                     

!item [Example:] (!T)!/author Dirk Hagedorn(!t)

!item [See:] (!I)!/maketitle(!i), (!I)!/authorimage(!i), Title page

!end_xlist
#------------------------------
!subsubnode !/docinfo [authorimage]

!begin_xlist [Type & position:]

!item [Type & position:]
	command, preamble

!item [Syntax:]
	(!T)!/authorimage <file>(!t)

!item [Description:]
	(!T)<file>(!t) will be displayed above the name of the author on the  
	title page if the destination format supports images.                 

!item [Example:] (!T)!/authorimage dh(!t)

!item [See:] (!I)!/maketitle(!i), (!I)!/author(!i), Title page, Images

!end_xlist
