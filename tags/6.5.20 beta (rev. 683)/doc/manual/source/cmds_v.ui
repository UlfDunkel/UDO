!begin_node (!kkkw [V] [v])
!html_name cmd_V().html
!label (!kk1 [/V])
!label (!kk1 [/v])

!iflang [german]
Klartext ausgeben.
!endif
!iflang [english]
Print preformatted text.
!endif

!begin_xlist [(!TYPEPOSITION)]

!item [(!TYPEPOSITION)] (!PLACEHOLDER), (!PREAMBLE) & (!MAINPART)

!item [(!SYNTAX)] (!T)(!/V)<(!TEXT)>(!/v)(!t)

!iflang [german]
!item [(!DESCRIPTION)]
""<text>"" wird als Klartext dargestellt, falls möglich.
Für (!LaTeX) wird ''(!V)\verb+<text>+'(!v)'' ausgegeben, bei allen
anderen Formaten erfolgt die gleiche Ausgabe wie bei
(!KKKW [T] [t]).
!endif
!iflang [english]
!item [(!DESCRIPTION)]
""<text>"" will be displayed preformatted if possible.
When converting to (!LaTeX), ''(!V)\verb+<text>+'(!v)'' will be printed,
otherwise the same output will be generated as for (!KKKW [T] [t]).
!endif

!iflang [german]
!item [(!EXAMPLE)]
(!T)(!/V)Klartext(!/v)(!t)
!endif
!iflang [english]
!item [(!EXAMPLE)]
(!T)(!/V)preformatted(!/v)(!t)
!endif

!iflang [german]
!item [(!SEEALSO)]
Schriftarten, (!link [Texthervorhebungen] [Texthervorhebungen]), (!KKKW [T] [t]), (!KW [tex_verb])
!endif
!iflang [english]
!item [(!SEEALSO)]
Text styles, (!link [Emphasizing text] [Emphasizing text]), (!KKKW [T] [t]), (!KW [tex_verb])
!endif

!end_xlist

!end_node

