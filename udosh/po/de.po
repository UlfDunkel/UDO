#.
#. * filters for file chooser.
#. * Filters are separated by newlines.
#. * In each filter, the display string is separated from the extension list by '|'.
#. * If a display string is not defined, the extension list is used.
#. * An extension list may specify several wildcard specifications separated by spaces.
#.
#: ../gtk/udogtk.h:69
msgid  ""
"*.exe *.bat *.cmd|Program files (*.exe;*.bat;*.cmd)\n"
"*.*|All files (*.*)\n"
msgstr ""
"*.exe *.bat *.cmd|Programm-Dateien (*.exe;*.bat;*.cmd)\n"
"*.*|Alle Dateien (*.*)\n"

#: ../gtk/udogtk.h:70
msgid  ""
"*.u*|UDO files (*.u*)\n"
"*.*|All files (*.*)\n"
msgstr ""
"*.u*|UDO Dateien (*.u*)\n"
"*.*|Alle Dateien (*.*)\n"

#: ../gtk/udogtk.h:71
msgid  "*.*|All files (*.*)\n"
msgstr "*.*|All Dateien (*.*)\n"

#: ../gtk/udogtk.h:72
msgid  ""
"*.hlp *.chm|Help files (*.hlp;*.chm)\n"
"*.*|All files (*.*)\n"
msgstr ""
"*.hlp *.chm|Hilfe-Dateien (*.hlp;*.chm)\n"
"*.*|Alle Dateien (*.*)\n"
